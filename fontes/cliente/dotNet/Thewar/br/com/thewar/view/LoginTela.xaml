        <Label Content="email:" Height="28" HorizontalAlignment="Left" Margin="20,12,0,0" Name="lbEmail" VerticalAlignment="Top" />
        <Label Content="senha:" Height="28" HorizontalAlignment="Left" Margin="17,46,0,0" Name="lbSenha" VerticalAlignment="Top" />
