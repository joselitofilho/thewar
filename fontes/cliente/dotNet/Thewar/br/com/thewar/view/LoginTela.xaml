        <TextBox Height="23" HorizontalAlignment="Left" Margin="67,46,0,0" Name="tbSenha" VerticalAlignment="Top" Width="120" />
        <Button Content="Login" Height="23" HorizontalAlignment="Left" Margin="112,75,0,0" Name="btLogin" VerticalAlignment="Top" Width="75" Click="btLogin_Click" />
    </Grid>
</UserControl>
